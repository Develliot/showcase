{
    "name": "showcase",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://develliot.github.io/showcase",
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/path/to/dir/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        },
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "coverageReporters": [
            "text"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "dependencies": {
        "@types/google-map-react": "^1.1.3",
        "@types/jest": "^24.0.23",
        "@types/node": "12.12.12",
        "@types/react": "16.9.13",
        "@types/react-dom": "16.9.4",
        "@types/react-router-dom": "^5.1.3",
        "@types/underscore": "^1.9.4",
        "axios": "^0.19.0",
        "google-map-react": "^1.1.5",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "typescript": "3.7.2",
        "underscore": "^1.9.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/styled-components": "^4.4.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "gh-pages": "^2.1.1",
        "prettier": "^1.19.1",
        "react-test-renderer": "^16.12.0",
        "styled-components": "^4.4.1",
        "ts-jest": "^24.2.0"
    }
}
